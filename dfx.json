{
  "version": 1,
  "canisters": {
    "ip_address_backend": {
      "type": "motoko",
      "main": "src/backend/main.mo",
      "candid": "src/backend/main.did"
    },
    "ip_address_frontend": {
      "type": "custom",
      "build": "npm run build",
      "root": "dist",
      "source": [
        "dist"
      ],
      "candid": "",
      "wasm": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  }
} 