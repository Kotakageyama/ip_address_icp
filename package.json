{
  "name": "ip_address_icp",
  "version": "1.0.0",
  "description": "ICP上でIPアドレスと位置情報を表示するFull on Chainサイト",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter ip-address-tracker-web dev",
    "build": "pnpm --filter ip-address-tracker-web build",
    "preview": "pnpm --filter ip-address-tracker-web preview",
    "start": "dfx start --background",
    "deploy": "pnpm build && dfx deploy",
    "deploy-local": "pnpm build && dfx deploy --network local",
    "deploy-ic": "pnpm build && dfx deploy --network ic",
    "stop": "dfx stop",
    "web:dev": "pnpm --filter ip-address-tracker-web dev",
    "web:build": "pnpm --filter ip-address-tracker-web build",
    "web:lint": "pnpm --filter ip-address-tracker-web lint",
    "web:format": "pnpm --filter ip-address-tracker-web format"
  },
  "dependencies": {
    "@dfinity/agent": "^0.19.2",
    "@dfinity/candid": "^0.19.2",
    "@dfinity/principal": "^0.19.2",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.4",
    "@types/node": "^24.0.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "keywords": [
    "internet-computer",
    "icp",
    "blockchain",
    "web3",
    "canister",
    "motoko",
    "geolocation",
    "react",
    "typescript",
    "vite"
  ],
  "author": "ICP Developer",
  "license": "MIT"
}
